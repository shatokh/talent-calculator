{
  "version": 2,

  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control",  "value": "public, max-age=3600" },
        { "key": "X-Robots-Tag",    "value": "index, follow" }
      ]
    }
  ],

  "env": {
    "NODE_ENV": "production"
  },

  "github": {

    "productionBranch": "master"
  }
}